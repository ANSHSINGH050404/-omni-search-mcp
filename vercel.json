{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://omni-search-mcp.onrender.com/api/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://omni-search-mcp.onrender.com"
  }
}
